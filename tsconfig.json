{
  "compilerOptions": {
    "target": "ESNext",
    "module": "Node16",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src", "vite.config.ts"]
}
